#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules/.pnpm/ts-node@10.4.0_43d2036524ce97aa8076ce68340fa9ec/node_modules/ts-node/dist/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules/.pnpm/ts-node@10.4.0_43d2036524ce97aa8076ce68340fa9ec/node_modules/ts-node/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules/.pnpm/ts-node@10.4.0_43d2036524ce97aa8076ce68340fa9ec/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules/.pnpm/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/node_modules:/mnt/c/Users/abzh4/projects/node_modules:/mnt/c/Users/abzh4/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules/.pnpm/ts-node@10.4.0_43d2036524ce97aa8076ce68340fa9ec/node_modules/ts-node/dist/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules/.pnpm/ts-node@10.4.0_43d2036524ce97aa8076ce68340fa9ec/node_modules/ts-node/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules/.pnpm/ts-node@10.4.0_43d2036524ce97aa8076ce68340fa9ec/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules/.pnpm/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/node_modules:/mnt/c/Users/abzh4/projects/node_modules:/mnt/c/Users/abzh4/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin-cwd.js" "$@"
else
  exec node  "$basedir/../../dist/bin-cwd.js" "$@"
fi
