#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules/.pnpm/acorn@8.7.0/node_modules/acorn/bin/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules/.pnpm/acorn@8.7.0/node_modules/acorn/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules/.pnpm/acorn@8.7.0/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules/.pnpm/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/node_modules:/mnt/c/Users/abzh4/projects/node_modules:/mnt/c/Users/abzh4/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules/.pnpm/ts-node@10.4.0_43d2036524ce97aa8076ce68340fa9ec/node_modules/ts-node/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules/.pnpm/ts-node@10.4.0_43d2036524ce97aa8076ce68340fa9ec/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules/.pnpm/acorn@8.7.0/node_modules/acorn/bin/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules/.pnpm/acorn@8.7.0/node_modules/acorn/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules/.pnpm/acorn@8.7.0/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules/.pnpm/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/node_modules:/mnt/c/Users/abzh4/projects/node_modules:/mnt/c/Users/abzh4/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules/.pnpm/ts-node@10.4.0_43d2036524ce97aa8076ce68340fa9ec/node_modules/ts-node/node_modules:/mnt/c/Users/abzh4/projects/graphql-production/express/node_modules/.pnpm/ts-node@10.4.0_43d2036524ce97aa8076ce68340fa9ec/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@8.7.0/node_modules/acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../../../../../acorn@8.7.0/node_modules/acorn/bin/acorn" "$@"
fi
